{
    "compilerOptions": {
        "jsx": "react-jsx",
        "allowJs": true,
        "paths": {
            "@/*": ["./src/*"],
            "@parts/*": ["./src/components/*"],
            "@c/*": ["./src/components/*"],
            "@i": ["./src/components/Icon"],
            "@contexts": ["./src/components/Contexts"],
            "@waffl": ["./src/theme/waffl"],
            "@theme": ["./src/theme"],
            "@theme/*": ["./src/theme/*"],
            "@tackl": ["./src/theme/tackl"],
            "@tackl/*": ["./src/theme/tackl/*"],
            "@utils/*": ["./src/utils/*"],
            "@public/*": ["./public/*"]
        }
    }
}
